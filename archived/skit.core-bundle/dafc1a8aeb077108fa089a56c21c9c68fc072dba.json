{
    "manifests": {
        "skit/core-bundle": {
            "manifest": {
                "bundles": {
                    "Skit\\Bundle\\CoreBundle\\SkitCoreBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "src/": "%SRC_DIR%/",
                    "var/": "%VAR_DIR%/"
                },
                "env": {
                    "TASKS_DATABASE_URL": "postgresql://skit-tasks:changeme@127.0.0.1:5432/skit_tasks_db?serverVersion=13&charset=utf8",
                    "MESSENGER_TASKS_TRANSPORT_DSN": "amqp://guest:guest@localhost:5672/%2f/messages?vhost=skit-tasks"
                }
            },
            "files": {
                "config/packages/skit.yaml": {
                    "contents": [
                        "skit_rest_helper:",
                        "  scan:",
                        "    - '%kernel.project_dir%/vendor/skit/core-bundle/src/Controller'",
                        "",
                        "framework:",
                        "  cache:",
                        "    pools:",
                        "      resource_schema_cache:",
                        "        adapter: cache.adapter.array",
                        "  messenger:",
                        "    transports:",
                        "      async_tasks:",
                        "        dsn: '%env(MESSENGER_TASKS_TRANSPORT_DSN)%'",
                        "        retry_strategy:",
                        "          max_retries: 0",
                        "    routing:",
                        "      'Skit\\Bundle\\CoreBundle\\Message\\ProcessTaskMessage': async_tasks",
                        "",
                        "doctrine:",
                        "  dbal:",
                        "    types:",
                        "      datetime_immutable_utc: Skit\\Bundle\\CoreBundle\\Component\\Resource\\Doctrine\\DBAL\\Types\\UTCDateTimeImmutable",
                        "    connections:",
                        "      tasks:",
                        "        url: '%env(resolve:TASKS_DATABASE_URL)%'",
                        "        mapping_types:",
                        "          enum: string",
                        "  orm:",
                        "    entity_managers:",
                        "      default:",
                        "        mappings:",
                        "          gedmo_translatable:",
                        "            type: annotation",
                        "            prefix: Gedmo\\Translatable\\Entity",
                        "            dir: \"%kernel.project_dir%/vendor/gedmo/doctrine-extensions/src/Translatable/Entity\"",
                        "            alias: GedmoTranslatable",
                        "            is_bundle: false",
                        "      tasks:",
                        "        connection: tasks",
                        "        naming_strategy: doctrine.orm.naming_strategy.underscore_number_aware",
                        "",
                        "",
                        "stof_doctrine_extensions:",
                        "  default_locale: en",
                        "  translation_fallback: true",
                        "  orm:",
                        "    default:",
                        "      translatable: true",
                        "",
                        "",
                        "when@prod:",
                        "  framework:",
                        "    cache:",
                        "      pools:",
                        "        resource_schema_cache:",
                        "          adapter: cache.adapter.system",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/skit.yaml": {
                    "contents": [
                        "skit:",
                        "  resource: '@SkitCoreBundle/config/routes.yaml'"
                    ],
                    "executable": false
                },
                "src/BatchAction/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/CronJob/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/Entity/Generated/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/MetricCollector/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/RowAction/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/Task/Entity/Generated/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "var/resource/config/.gitkeep": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "dafc1a8aeb077108fa089a56c21c9c68fc072dba"
        }
    }
}
